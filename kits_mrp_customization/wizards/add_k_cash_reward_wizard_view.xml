<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data noupdate="0">

        <record id="add_k_cash_reward_wizard_form" model="ir.ui.view">
            <field name="name">add.kcash.reward.wizard.form</field>
            <field name="model">add.kcash.reward.wizard</field>
            <field name="arch" type="xml">
                <form string="">
                    <sheet>
                        <group>
                            <group>
                                <field name="expiry_date"/>
                            </group>
                            <group>
                                <field name="amount"/>
                            </group>
                        </group>
                        <footer>
                            <button string="Process" class="btn btn-primary" type="object" name="action_process" />
                            <button string="Cancel" class="btn-secondary" special="cancel" />
                        </footer>
                    </sheet>
                </form>
            </field>
        </record>

    </data>
</odoo>
